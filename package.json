{
  "name": "CocktailDirectory",
  "version": "0.0.1",
  "description": "simple cocktail directory application",
  "scripts": {
    "setup-app": "chmod +x scripts/setup_app.sh && ./scripts/setup_app.sh",
    "serve-app": "chmod +x scripts/serve_app.sh && ./scripts/serve_app.sh",
    "generate-docs": "chmod +x scripts/generate_docs.sh && ./scripts/generate_docs.sh",
    "dev-flex-card-driver": "tsc ./src/flex_card_driver.ts --outFile ./pub/static/flex_card_driver.js",
    "dev-slide-card-driver": "tsc ./src/slide_card_driver.ts --outFile ./pub/static/slide_card_driver.js",
    "clean": "chmod +x scripts/cleanup.sh && ./scripts/cleanup.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jesssullivan/CocktailDirectory.git"
  },
  "author": "Jess Sullivan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jesssullivan/CocktailDirectory/issues"
  },
  "homepage": "https://github.com/Jesssullivan/CocktailDirectory#readme",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "ts-node": "^10.7.0",
    "typescript-require": "^0.3.0",
    "typescript": "^4.6.4"
  }
}